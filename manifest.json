{
    "id": "@hardbulls/app",
    "name": "Bulls App",
    "short_name": "Bulls App",
    "description": "Hard Bulls Baseball App",
    "start_url": "/",
    "display": "standalone",
    "background_color": "#ffffff",
    "theme_color": "#ed1c24",
    "orientation": "portrait",
    "icons": [
      {
        "src": "/192x192.png",
        "sizes": "192x192",
        "type": "image/png",
        "purpose": "any"
      },
      {
        "src": "/192x192-maskable.png",
        "sizes": "192x192",
        "type": "image/png",
        "purpose": "maskable"
      },
      {
        "src": "/512x512.png",
        "sizes": "512x512",
        "type": "image/png",
        "purpose": "any"
      },
      {
        "src": "/512x512-maskable.png",
        "sizes": "512x512",
        "type": "image/png",
        "purpose": "maskable"
      }
    ],
    "launch_handler": {
        "client_mode": ["navigate-existing", "auto"]
    },
    "dir": "auto",
    "display_override": ["window-controls-overlay", "standalone", "browser"],
    "scope": "/",
    "scope_extensions": [
      { "origin": "*.hardbulls.com" }
    ],
    "prefer_related_applications": false,
    "related_applications": [],
    "categories": ["sports", "entertainment"],
     "lang": "de"
  }
